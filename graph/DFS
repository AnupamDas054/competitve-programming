#include<bits/stdc++.h>

using namespace std;

int n,m,x,y;
vector<int>edge[200000];
int vis[200000];
void dfs(int pos)
{

    if(vis[pos]) return;
    vis[pos]=1;
    cout<<pos<<endl;
    for(int i=0;i<edge[pos].size();i++)
    {
        dfs(edge[pos][i]);
    }
}

int main()
{

    cin>>n>>m;
    for(int i=0;i<m;i++)
    {
        cin>>x>>y;
        edge[x].push_back(y);
        edge[y].push_back(x);
    }
    dfs(1);
}



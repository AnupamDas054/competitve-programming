#how many of a node 

bool vis[sz];

void dfs(ll n)
{
	vis[n] = true;
	child[n] = 1;

	for(int i = 0; i < v[n].size(); i++)
	{
		ll u = v[n][i];

		if(vis[u]==false)
		{
			dis[u] = dis[n]+1;

			dfs(u);

			child[n] += child[u];
		}
	}
}

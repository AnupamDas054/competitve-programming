#include<bits/stdc++.h>

using namespace std;
int timer;
vector<int>adj[100005];
int vis[100005],low[100005],in[100005];
void bridge(int node,int par)
{
    vis[node]=1;
    in[node]=low[node]=timer;
    timer++;
    for(auto it: adj[node])
    {
        if(it==par)
        {
            continue;
        }
        if(vis[it]==0)
        {
            //forwared edge
            bridge(it,node);
            if(low[it]>in[node])
            {
                cout<<node<<" "<<it<<" is  a bridge"<<endl;
            }
            low[node]=min(low[node],low[it]);

        }
        else
        {
            low[node]=min(low[node],in[it]);
        }
    }
}

int main()
{
    int n,m;cin>>n>>m;
    int x,y;
    for(int i=0;i<m;i++)
        cin>>x>>y,adj[x].push_back(y),adj[y].push_back(x);
    bridge(1,-1);

}

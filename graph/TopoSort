#include<bits/stdc++.h>
using namespace std;

vector<int>edge[200000];
int n,m,x,y,z;
int indegree[200000];
int main()
{
    cin>>n>>m;
    for(int i=0; i<m; i++)
    {
        cin>>x>>y;
        edge[x].push_back(y);
        indegree[y]++;

    }
    ///////////////// BFS Part ?????//////////////////////
    queue<int>q;

    for(int i=1;i<=n;i++) if(indegree[i]==0) q.push(i);
    while(q.size()>0)
    {
            int k=q.front();
            q.pop();
            cout<<k<<" ";
            for(int i=0;i<edge[k].size();i++)
            {
                int to=edge[k][i];
                indegree[to]--;
                if(indegree[to]==0) q.push(to);
            }

    }
    cout<<endl;
    return 0;
}
